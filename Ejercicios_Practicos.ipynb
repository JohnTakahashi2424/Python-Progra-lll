        "            return\n",
        "        tabla = acumulacion_meses_por_mes(n)\n",
        "        mostrar_tabla(tabla)\n",
        "        total = (n * (n + 1) * (n + 2)) // 6\n",
        "        print(f\"\\nAcumulaci칩n total hasta el mes {n}: {total} meses\\n\")\n",
        "    except ValueError:\n",
        "        print(\"Entrada inv치lida. Por favor ingrese un n칰mero entero.\")\n",
        "\n",
        "iniciar()"
