        "def mostrar_tabla(tabla):\n",
        "    print(\"\\nTabla de acumulaci칩n de meses:\\n\")\n",
        "    print(f\"{'Mes':<6} | {'Acumulado':<10}\")\n",
        "    print(\"-\" * 22)\n",
        "    for mes, acumulado in tabla:\n",
        "        print(f\"{mes:<6} | {acumulado:<10}\")\n",
        "    print(\"-\" * 22)\n",
        "\n",
        "def iniciar():\n",
        "    print(\"Calculador de Meses Acumulados\\n\")\n",
        "    try:\n",
